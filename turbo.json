{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**", "build/**", ".docusaurus/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "test": {
      "dependsOn": ["^build"]
    },
    "type-check": {
      "dependsOn": ["^build"]
    },
    "docs#build": {
      "outputs": ["build/**", ".docusaurus/**"],
      "dependsOn": ["^build"]
    },
    "docs#dev": {
      "cache": false,
      "persistent": true
    }
  }
}